{
  "name": "bukabengkel",
  "private": true,
  "version": "0.0.1",
  "description": "BukaBengkel API Service",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "startConsumer": "nest start -c nest-cli-consumer.json ",
    "startConsumer:dev": "nest start --watch -c nest-cli-consumer.json ",
    "startConsumer:debug": "nest start --debug --watch -c nest-cli-consumer.json ",
    "startConsumer:prod": "node dist/consumer",
    "cli": "ts-node ./src/cli.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "npx sequelize-cli db:migrate",
    "migrateUndo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all"
  },
